<div>
   	{{#each burgers}}
   		{{#unless devoured}}
		   	{{burger_name}}
			<button class="devoured" data-id="{{id}}" data-newsleep="{{devoured}}">Devour Me!!!!
			</button>
			<br>
   		{{/unless}}
  	{{/each}}
</div>
<form class="create-form">
  	<div class="form-group">
    	<input type="text" id="ca" name="burger_name">
		<br>
    	
  		<button type="submit" class="mainBtn">submit</button>
  		<p class="specialFont" >Enter the burger you want to eat.<br>
    	                    Click the submit button when done.</p>
	</div>						
</form>
